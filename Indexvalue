package week3.day1;

public class Assignment7 {

	public static void main(String[] args) {
	
		
		String text = "Java Exercise";
		
		// Index for character E and s 
		
		char e1 = text.charAt(5);
		System.out.println(" fifth Index value " +e1);
		
		char e2 = text.charAt(11);
		System.out.println(" 11th Index value " +e2);
		
	}

}
